<ul>
<% @attendees.each do |attendee| %>
  <li>
    <%= attendee.name %>
    <%= link_to 'show', event_attendee_path(@event, attendee) %>
    <%= link_to 'edit', edit_event_attendee_path(@event, attendee) %>
    <%= link_to 'destroy', event_attendee_path(@event, attendee), :method => :delete %>
  </li>
<% end %>

</ul>
<dev>
    <li><%= link_to 'new attendee', new_event_attendee_path(@event) %> </li>
    <li><%= link_to '<<back_to_events',events_path %></li>
</dev>
